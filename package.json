{
  "name": "route-analysis-dashboard",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/route-analyzer.js",
  "bin": {
    "route-analyzer": "dist/cli.js"
  },
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/cli.js",
    "time-breakdown": "npm run build && node dist/time-breakdown.js",
    "store-analysis": "npm run build && node dist/store-analysis.js",
    "returns-breakdown": "npm run build && node dist/returns-breakdown.js",
    "build": "tsc",
    "postinstall": "python3 -m venv venv && ./venv/bin/python3 -m pip install -r requirements.txt && npm run build",
    "lint": "echo 'Linting...' && exit 0",
    "test": "tsx --test tests/**/*.test.ts",
    "validate": "npm run lint && npm test && npm run build",
    "prp": "node scripts/context-engine.mjs prp",
    "scout": "node scripts/context-engine.mjs scout",
    "implement": "node scripts/context-engine.mjs implement",
    "sync": "node scripts/context-engine.mjs sync"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
